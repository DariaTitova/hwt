@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    <div class="col-md-5">
        <div class="well no-padding">
            <div>
                <ul class="nav nav-list nav-menu-list-style">

                    <li>
                         @if (ViewBag.Createble != null)
                        {
                            <div class="dropdown">
                                <button class="btn btn-primary dropdown-toggle" type="button" data-toggle="dropdown">
                                    Добавить
                                </button>
                                <ul class="dropdown-menu">
                                    @foreach (var create in ViewBag.Createble)
                                    {
                                        <li><a href="#" onclick="openPartial('@create.Value')">@create.Key</a></li>

                                    }
                                </ul>
                            </div>
                        }

                    </li>
                    <li id="menyPartial"     >
                             @{
                                Html.RenderPartial("~/Views/Home/MenyPartial.cshtml");
                            }
                     </li>
                </ul>
            </div>
        </div>
    </div>

    <div class="col-md-7" id="linkResult" style="padding:20px;">
        @{
            Html.RenderPartial("~/Views/Clauses/ClausesShow.cshtml");
        }
    </div>
</div>

<script src="~/Scripts/Openpartitial.js" language="javascript" type="text/javascript"></script>
<script src="~/Scripts/jquery.unobtrusive-ajax.js" language="javascript" type="text/javascript"></script>












